<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.micron.myapplication" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShowHospitalList"
            android:label="@string/title_activity_show_hospital_list" >
        </activity>
        <activity
            android:name=".ShowHealthList"
            android:label="@string/title_activity_show_health_list" >
        </activity>
        <activity
            android:name=".DoctorSearchActivity"
            android:label="@string/title_activity_doctor_search" >
        </activity>
        <activity
            android:name=".HospitalSearchActivity"
            android:label="@string/title_activity_hospital_search" >
        </activity>
        <activity
            android:name=".ShowDetailNews"
            android:label="@string/title_activity_show_detail_news" >
        </activity>
        <activity
            android:name=".DashboardAcitivity"
            android:label="@string/title_activity_dashboard_acitivity" >
        </activity>
        <activity
            android:name=".HealthTipsActivity"
            android:label="@string/title_activity_health_tips" >
        </activity>
        <activity
            android:name=".HealthnewsActivity"
            android:label="@string/title_activity_healthnews" >
        </activity>
        <activity
            android:name=".LatestnewsActivity"
            android:label="@string/title_activity_latestnews" >
        </activity>
    </application>

</manifest>
